{
  "name": "org-application",
  "projectType": "application",
  "sourceRoot": "apps/org-application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/org-application"
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "org-application:build",
        "dev": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/org-application/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
